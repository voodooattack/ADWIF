                "type": "object",
                "properties": {
                  "fgcolour": {
                    "type":"string",
                    "oneOf": ["Default", "Black", "Red", "Green", "Yellow", "Blue", "Magenta", "Cyan", "White"],
                    "caseSensetive":false
                  },
                  "bgcolour": {
                    "type":"string",
                    "oneOf": ["Default", "Black", "Red", "Green", "Yellow", "Blue", "Magenta", "Cyan", "White"],
                    "caseSensetive":false
                  },
                  "attributes": {
                    "type":"array",
                    "items":{
                      "type":"string",
                      "oneOf": ["Normal", "Bold", "Dim", "Underline", "StandOut", "AltCharSet"],
                      "caseSensetive":false
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "required": [ "name", "disp", "style" ]
  }
